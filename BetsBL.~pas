unit BetsBL;

interface

uses Windows;

function Bets():BOOL;

implementation

uses BetsColumnBL, BetsDozenBL,
      BetsNumberBL, BetsSixlineBL, BetsStreetBL, BetsAngleBL;

function Bets():BOOL;
begin
  BetsColumn;
  BetsDozen;
  BetsNumber;
  BetsSixline;
  BetsStreet;
  BetsAngle;

  Bets := True;
end;

end.
